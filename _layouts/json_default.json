[
  {% for post in site.posts %}
      {
        "title"    : {{ post.title | jsonify }},
        "url"     : {{ post.url | jsonify }},
        "date"     : {{ post.date | date: "%B %d, %Y" | jsonify }},
        "content"  : {{ post.content | escape | jsonify }}
      } {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]
